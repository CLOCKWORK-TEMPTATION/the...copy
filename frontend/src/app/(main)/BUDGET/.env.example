# Gemini API Configuration
# احصل على مفتاح API من https://makersuite.google.com/app/apikey
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_API_KEY=your_gemini_api_key_here
API_KEY=your_gemini_api_key_here

# Database Configuration (إذا كنت تستخدم قاعدة بيانات)
DATABASE_URL="postgresql://user:password@localhost:5432/filmbudget"

# NextAuth Configuration (إذا كنت تستخدم المصادقة)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Application Configuration
NODE_ENV=development
PORT=3000
