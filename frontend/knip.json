{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/app/layout.tsx", "src/app/page.tsx"],
  "project": ["src/**/*.ts", "src/**/*.tsx"],
  "ignore": [
    "**/node_modules/**",
    "**/.next/**",
    "**/dist/**",
    "**/build/**",
    "**/*.config.{js,ts}",
    "**/.vercel/**",
    "**/.vscode/**",
    "**/coverage/**",
    "**/__tests__/**",
    "**/*.test.{ts,tsx,js,jsx}",
    "**/*.spec.{ts,tsx,js,jsx}",
    "**/__smoke__/**",
    "**/api/critique/**"
  ]
}
